    #     api_response, http_status_code, response_headers = api_instance.get_vault_component_by_id_with_http_info(id)
    #     pprint(api_response)
    #     pprint(http_status_code)
    #     pprint(response_headers)
    # except fds.sdk.Vault.ApiException as e:
    #     print("Exception when calling ComponentsApi->get_vault_component_by_id: %s\n" % e)

    # Get response asynchronous
