{
  "language": "zh-tw",
  "intents": {
    "HassTurnOff": {
      "data": [
        {
          "sentences": [
            "<turn_off> [所有|全部] <area>[的][所有|全部]<fan>",
            "[<let>][所有|全部][的]<area>[的][所有|全部]<fan><turn_off>",
            "<turn_off> <area> (風扇|吊扇)",
            "<let>  <area> [<all>] (風扇 | 吊扇) <turn_off>"
          ],
          "slots": {
            "domain": "fan",
            "name": "all"
          },
          "response": "fans_area"
        },
        {
          "sentences": [
            "<turn_off> [<area>] <name>",
            "[<let>] [<area>] <name> <turn_off>"
          ],
          "excludes_context": {
            "domain": [
              "cover",
              "scene",
              "sensor",
              "script"
            ]
          }
        },
        {
          "sentences": [
            "<turn_off><area>[<all>]<light>",
            "<turn_off>[<all>]<area><light>",
            "[<let>]<area>[<all>]<light><turn_off>",
            "[<let>][<all>]<area><light><turn_off>"
          ],
          "slots": {
            "domain": "light"
          },
          "response": "lights_area"
        },
        {
          "sentences": [
            "<close> [<area>] <name>",
            "<let> [<area>] <name> <close>"
          ],
          "requires_context": {
            "domain": "cover"
          },
          "response": "cover"
        },
        {
          "sentences": [
            "<let> 車庫門 <close>",
            "<close> 車庫門"
          ],
          "slots": {
            "domain": "cover",
            "device_class": "garage"
          },
          "response": "cover_device_class"
        },
        {
          "sentences": [
            "<close> [<area>] [的] {cover_classes:device_class}",
            "<let> [<area>] [的] {cover_classes:device_class} <close>"
          ],
          "slots": {
            "domain": "cover"
          },
          "response": "cover_device_class"
        }
      ]
    },
    "HassTurnOn": {
      "data": [
        {
          "sentences": [
            "[啟動]<name>[場景]"
          ],
          "requires_context": {
            "domain": "scene"
          },
          "slots": {
            "domain": "scene"
          },
          "response": "scene"
        },
        {
          "sentences": [
            "<turn_on><area>[<all>]<light>",
            "<turn_on>[<all>]<area><light>",
            "[<let>]<area>[<all>]<light><turn_on>",
            "[<let>][<all>]<area><light><turn_on>"
          ],
          "slots": {
            "domain": "light"
          },
          "response": "lights_area"
        },
        {
          "sentences": [
            "<open> [<area>] <name>",
            "<let> [<area>] <name> <open>"
          ],
          "requires_context": {
            "domain": "cover"
          },
          "response": "cover"
        },
        {
          "sentences": [
            "<let> 車庫門 <open>",
            "<open> 車庫門"
          ],
          "slots": {
            "domain": "cover",
            "device_class": "garage"
          },
          "response": "cover_device_class"
        },
        {
          "sentences": [
            "<open> <area> {cover_classes:device_class}",
            "<let> <area> {cover_classes:device_class} <open>"
          ],
          "slots": {
            "domain": "cover"
          },
          "response": "cover_device_class"
        },
        {
          "sentences": [
            "<turn_on><area>(風扇|吊扇)"
          ],
          "slots": {
            "domain": "fan",
            "name": "all"
          },
          "response": "fans_area"
        },
        {
          "sentences": [
            "[(執行|開始)]<name>[腳本]"
          ],
          "requires_context": {
            "domain": "script"
          },
          "slots": {
            "domain": "script"
          },
          "response": "script"
        },
        {
          "sentences": [
            "<turn_on> [<area>] <name>",
            "[<let>] [<area>] <name> <turn_on>"
          ],
          "excludes_context": {
            "domain": [
              "cover",
              "scene",
              "sensor",
              "script"
            ]
          }
        }
      ]
    },
    "HassGetState": {
      "data": [
        {
          "sentences": [
            "[<area>] [(目前 | 現在)] [的] <name> [(目前 | 現在)] [狀態]"
          ],
          "response": "one",
          "excludes_context": {
            "domain": [
              "scene",
              "script"
            ]
          }
        },
        {
          "sentences": [
            "[<area>] [(目前 | 現在)] <name> [(目前 | 現在)] [狀態] (<what_is> | <how_many_is>)"
          ],
          "response": "one"
        },
        {
          "sentences": [
            "[<area>] <name> [是] {on_off_states:state} [嗎]"
          ],
          "response": "one_yesno",
          "excludes_context": {
            "domain": [
              "cover"
            ]
          }
        },
        {
          "sentences": [
            "[<area>] <name> [(是 | 是不是)] {on_off_states:state}"
          ],
          "response": "one_yesno",
          "excludes_context": {
            "domain": [
              "cover"
            ]
          }
        },
        {
          "sentences": [
            "[<area>] [(有 | 有沒有)] [任何] [的] {on_off_domains:domain} [(是 | 是不是)] {on_off_states:state} [著] [的] [嗎]"
          ],
          "response": "any"
        },
        {
          "sentences": [
            "[<area>] [<all>] {on_off_domains:domain} [(是 | 是不是)] [都是] {on_off_states:state} [著] [的] [嗎]"
          ],
          "response": "all"
        },
        {
          "sentences": [
            "[<area>] <which> [<area>] {on_off_domains:domain} [是] {on_off_states:state} [著] [的] [嗎]"
          ],
          "response": "which"
        },
        {
          "sentences": [
            "[<area>] <how_many> [(盞 | 扇 | 個)] [<area>] {on_off_domains:domain} [是] {on_off_states:state} [著] [的] [嗎]"
          ],
          "response": "how_many"
        },
        {
          "sentences": [
            "[<area>] <name> [(是 | 是不是)] {cover_states:state} [的] [嗎]"
          ],
          "response": "one_yesno",
          "requires_context": {
            "domain": "cover"
          },
          "slots": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "[<area>] [(有 | 有沒有 | 有任何)] {cover_classes:device_class} 是 {cover_states:state} [的] [嗎]"
          ],
          "response": "any",
          "slots": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "[<area>] [<all>] {cover_classes:device_class} [(都是 | 是 | 是不是)] {cover_states:state} [的] [嗎]"
          ],
          "response": "all",
          "slots": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "[<area>] [<which>] {cover_classes:device_class} [是] {cover_states:state} [的] [嗎]"
          ],
          "response": "which",
          "slots": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "[<area>] <how_many> [扇] {cover_classes:device_class} 是 {cover_states:state}"
          ],
          "response": "how_many",
          "slots": {
            "domain": "cover"
          }
        },
        {
          "sentences": [
            "[<area>][<name>][(是|是不是)]{lock_states:state}[著][(的|了)][嗎]"
          ],
          "response": "one_yesno",
          "requires_context": {
            "domain": "lock"
          },
          "slots": {
            "domain": "lock"
          }
        },
        {
          "sentences": [
            "[<area>][(有 | 有沒有 | 有任何)][<door>][是] [沒有]{lock_states:state}[著][的][嗎]"
          ],
          "response": "any",
          "slots": {
            "domain": "lock"
          }
        },
        {
          "sentences": [
            "[<area>][<all>][<door>][(都|都是|是|是不是)]{lock_states:state}[著][(的|了)][嗎]"
          ],
          "response": "all",
          "slots": {
            "domain": "lock"
          }
        },
        {
          "sentences": [
            "[<area>] [<which>][<door>][是] {lock_states:state}[著][(的|了)][嗎]"
          ],
          "response": "which",
          "slots": {
            "domain": "lock"
          }
        },
        {
          "sentences": [
            "[<area>]<how_many>[(扇|道)][<door>][是]{lock_states:state}[著][的][嗎]"
          ],
          "response": "how_many",
          "slots": {
            "domain": "lock"
          }
        }
      ]
    }
  },
  "settings": {
    "ignore_whitespace": true
  },
  "responses": {
    "errors": {
      "no_intent": "拍謝，我沒聽很懂",
      "no_area": "「{{ area }}」這個區域並不存在",
      "no_domain": "{{ area }}裡面沒有任何{{ domain }}",
      "no_device_class": "{{ area }}裡面並沒有任何{{ device_class }}",
      "no_entity": "「{{ entity }}」這個實體並不存在",
      "handle_error": "處理您的旨意時，發生了意外錯誤"
    },
    "intents": {
      "HassGetState": {
        "one": "{{ slots.name | capitalize }}為 {{ state.state_with_unit }}\n",
        "one_yesno": "{% if query.matched: %}\n  是的\n{% else: %}\n  沒有、{{ slots.name }}是{{ state.state_with_unit }}的\n{% endif %}\n",
        "any": "{% if query.matched %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {% if match|length > 4 %}\n    是的、{{ match[:3]|join(\"、\") }}及另外{{ (match|length - 3) }}個\n  {%- else -%}\n    是的、{% for name in match -%}{% if not loop.first and not loop.last %}、{% elif loop.last and not loop.first %}及{% endif -%}{{ name }}\n    {%- endfor -%}\n  {% endif %}\n{% else %}\n  沒有\n{% endif %}\n",
        "all": "{% if not query.unmatched: %}\n  是的\n{% else %}\n  {% set no_match = query.unmatched | map(attribute=\"name\") | sort | list %}\n  {% if no_match | length > 4 %}\n    沒有、{{ no_match[:3]|join(\"、\") }}及另外{{ (no_match|length - 3) }}個不是\n  {%- else -%}\n    沒有、{% for name in no_match -%}{% if not loop.first and not loop.last %}、{% elif loop.last and not loop.first %}及{% endif -%}{{ name }}{%- endfor %}不是\n  {% endif %}\n{% endif %}\n",
        "which": "{% if not query.matched %}\n  沒有任何\n{% else: %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {% if match | length > 4 %}\n    {{ match[:3]|join(\"、\") }}及另外{{ (match | length - 3) }}個\n  {% else %}\n    {%- for name in match -%}\n      {% if not loop.first and not loop.last %}、{% elif loop.last and not loop.first %}及{% endif -%}\n      {{ name }}\n    {%- endfor -%}\n  {% endif %}\n{% endif %}\n",
        "how_many": "{{ query.matched | length }}\n"
      },
      "HassTurnOff": {
        "default": "{{ state.domain }}已關閉",
        "lights_area": "燈光已關閉",
        "fans_area": "風扇已關閉",
        "cover": "已關閉",
        "cover_device_class": "{{ slots.device_class }}已關閉"
      },
      "HassTurnOn": {
        "default": "{{ state.domain }}已打開",
        "lights_area": "燈光已打開",
        "fans_area": "風扇已打開",
        "cover": "已打開",
        "cover_device_class": "{{ slots.device_class }}已打開",
        "scene": "已啟用",
        "script": "已開始"
      }
    }
  },
