        },
        {
          "in": "crna(a|o|u)",
          "out": "black"
        },
        {
          "in": "crven(a|o|u)",
          "out": "red"
        },
        {
          "in": "narandžast(a|o|u)",
          "out": "orange"
        },
        {
          "in": "žut(a|o|u)",
          "out": "yellow"
        },
        {
          "in": "zelen(a|o|u)",
