    "brightness": {
      "range": {
        "type": "percentage",
        "from": 0,
        "to": 100
      }
    },
    "brightness_level": {
      "values": [
        {
          "in": "(maksimum|najjače|maksimalnu|maksimalno)",
          "out": 100
        },
        {
          "in": "(minimum|minimalnu|minimalno|najslabije|najmanje)",
          "out": 1
        }
      ]
    },
    "temperature": {
      "range": {
        "type": "temperature",
        "from": 0,
        "to": 100
      }
    },
    "temperature_unit": {
      "values": [
        "celsius",
        {
          "in": "c|celzijus|celzijusa|celzija|celzijev|celzijevih|celczijeva",
          "out": "celsius"
        },
        "fahrenheit",
