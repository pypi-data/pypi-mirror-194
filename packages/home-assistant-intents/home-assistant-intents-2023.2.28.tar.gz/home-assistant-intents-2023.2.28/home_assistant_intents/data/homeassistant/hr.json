{
  "language": "hr",
  "intents": {
    "HassTurnOff": {
      "data": [
        {
          "sentences": [
            "<isključi> {name}",
            "<isključi> {name} [u|na] {area}"
          ],
          "response": "default_name",
          "excludes_context": {
            "domain": [
              "cover"
            ]
          }
        },
        {
          "sentences": [
            "<isključi> [svjetlo|svjetla|rasvjetu] [u|na] [prostoriji] <area>",
            "<isključi> [sva] [svjetl(a|o)] [u] <area> [svjetla|rasvjetu]"
          ],
          "slots": {
            "domain": "light"
          },
          "response": "lights_area"
        },
        {
          "sentences": [
            "<zatvori> <sve> {cover_classes:device_class} [u|na] [<area>]",
            "<zatvori> <area> {cover_classes:device_class}"
          ],
          "slots": {
            "domain": "cover"
          },
          "response": "cover_device_class"
        },
        {
          "sentences": [
            "<zatvori> <name>"
          ],
          "requires_context": {
            "domain": "cover"
          },
          "slots": {
            "device_class": [
              "awning",
              "blind",
              "curtain",
              "door",
              "garage",
              "gate",
              "shade",
              "shutter",
              "window"
            ]
          },
          "response": "cover_single"
        },
        {
          "sentences": [
            "<zatvori> <name> [u|na] {area}"
          ],
          "requires_context": {
            "domain": "cover"
          },
          "slots": {
            "device_class": [
              "awning",
              "blind",
              "curtain",
              "door",
              "garage",
              "gate",
              "shade",
              "shutter",
              "window"
            ]
          },
          "response": "cover_single"
        }
      ]
    },
    "HassGetState": {
      "data": [
        {
          "sentences": [
            "(koja je|koji je|reci mi) [status|vrijednost] <name> [[u|na] <area>]"
          ],
          "response": "one",
          "excludes_context": {
            "domain": [
              "scene",
              "script"
            ]
          }
        },
        {
          "sentences": [
            "(je li|da li je|postoji li) <name> {on_off_states:state} [[u|na] <area>]"
          ],
          "response": "one_yesno",
          "excludes_context": {
            "domain": [
              "cover"
            ]
          }
        },
        {
          "sentences": [
            "(je li|da li je|ima li|postoji li) [[bilo] koj[i|a|e]] {on_off_domains:domain} {on_off_states:state} [[u|na] <area>]"
          ],
          "response": "any"
        },
        {
          "sentences": [
            "(je li|da li je|ima li|postoji li) [[bilo] koj[i|a|e]] {on_off_domains:domain} [[u|na] <area>] {on_off_states:state}"
          ],
          "response": "any"
        },
        {
          "sentences": [
            "(je li|da li je|ima li|postoji li) [[bilo] koj[i|a|e]] {on_off_states:state} {on_off_domains:domain} [[u|na] <area>]"
          ],
          "response": "any"
        },
        {
          "sentences": [
            "(je li|jesu li) {on_off_domains:domain} {on_off_states:state} [in <area>]"
          ],
          "response": "all"
        },
        {
          "sentences": [
            "(koji|koja) [su] {on_off_domains:domain} [su] {on_off_states:state} [[u|na] <area>]"
          ],
          "response": "which"
        },
        {
          "sentences": [
            "koliko je ({on_off_states:state} {on_off_domains:domain}|{on_off_domains:domain} {on_off_states:state}) [[u|na] <area>]"
          ],
          "response": "how_many"
        }
      ]
    },
    "HassTurnOn": {
      "data": [
        {
          "sentences": [
            "<uključi> [svjetlo|svjetla|rasvjetu] [u|na] [prostoriji] <area>",
            "<uključi> [sva] [svjetl(a|o)] [u] <area> [svjetla|rasvjetu]"
          ],
          "slots": {
            "domain": "light"
          },
          "response": "lights_area"
        },
        {
          "sentences": [
            "<otvori> <sve> {cover_classes:device_class} [u|na] [<area>]",
            "<otvori> <area> {cover_classes:device_class}"
          ],
          "slots": {
            "domain": "cover"
          },
          "response": "cover_device_class"
        },
        {
          "sentences": [
            "<otvori> <name>"
          ],
          "requires_context": {
            "domain": "cover"
          },
          "slots": {
            "device_class": [
              "awning",
              "blind",
              "curtain",
              "door",
              "garage",
              "gate",
              "shade",
              "shutter",
              "window"
            ]
          },
          "response": "cover_single"
        },
        {
          "sentences": [
            "<otvori> <name> [u|na] {area}"
          ],
          "requires_context": {
            "domain": "cover"
          },
          "slots": {
            "device_class": [
              "awning",
              "blind",
              "curtain",
              "door",
              "garage",
              "gate",
              "shade",
              "shutter",
              "window"
            ]
          },
          "response": "cover_single"
        },
        {
          "sentences": [
            "(<uključi>|<otvori>) {name}",
            "(<uključi>|<otvori>) {name} [u|na] {area}"
          ],
          "response": "default_name",
          "excludes_context": {
            "domain": [
              "cover"
            ]
          }
        }
      ]
    }
  },
  "responses": {
    "errors": {
      "no_intent": "Nažalost, ne razumijem zahtjev",
      "no_area": "Prostorija s imenom {{ area }} ne postoji",
      "no_domain": "Ne postoji {{ domain }} u prostoriji {{ area }}",
      "no_device_class": "Prostorija {{ area }} ne sadrži {{ device_class }}",
      "no_entity": "Ne postoji uređaj ili entitet pod imenom {{ entity }}",
      "handle_error": "Došlo je do neočekivane pogreške prilikom obrade zahtjeva"
    },
    "intents": {
      "HassGetState": {
        "one": "{{ slots.name | capitalize }} je {{ state.state_with_unit }}\n",
        "one_yesno": "{% if query.matched %}\n  Da\n{% else %}\n  Ne\n{% endif %}\n",
        "any": "{% if query.matched %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {% if match | length > 4 %}\n    Da, {{ match[:3] | join(\", \") }} i još {{ (match | length - 3) }}\n  {%- else -%}\n    Da,\n    {% for name in match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} i {% endif -%}\n      {{ name }}\n    {%- endfor -%}\n  {% endif %}\n{% else %}\n  Ne\n{% endif %}\n",
        "all": "{% if not query.unmatched: %}\n  Da\n{% else %}\n  {% set no_match = query.unmatched | map(attribute=\"name\") | sort | list %}\n  {% if no_match | length > 4 %}\n    Ne, nisu {{ no_match[:3] | join(\", \") }} i još {{ (no_match | length - 3) }}\n  {%- else -%}\n    Ne,\n      {% if no_match | length > 1 %}\n        nisu\n      {%- else -%}\n        nije\n      {% endif %}\n    {% for name in no_match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} i {% endif -%}\n      {{ name }}\n    {%- endfor %}\n  {% endif %}\n{% endif %}\n",
        "which": "{% if not query.matched %}\n  Nema\n{% else: %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {% if match | length > 4 %}\n    {{ match[:3] | join(\", \") }} and {{ (match | length - 3) }} more\n  {% else %}\n    {%- for name in match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} i {% endif -%}\n      {{ name }}\n    {%- endfor -%}\n  {% endif %}\n{% endif %}\n",
        "how_many": "{{ query.matched | length }}\n"
      },
      "HassTurnOff": {
        "default": "Gasim {{ state.domain }}",
        "default_name": "Gasim {{ slots.name }}",
        "lights_area": "Svjetla su ugašena",
        "fans_area": "Ventiraltor je ugašen",
        "cover_device_class": "Zatvaram {{ slots.device_class }}",
        "cover_single": "Zatvaram {{ slots.name }}"
      },
      "HassTurnOn": {
        "default": "Palim {{ state.domain }}",
        "default_name": "Palim {{ slots.name }}",
        "lights_area": "Svjetla su upaljena",
        "fans_area": "Ventilatori su upaljeni",
        "cover_single": "Otvaram {{ slots.name }}",
        "cover_device_class": "Otvaram {{ slots.device_class }}"
      }
    }
  },
  "lists": {
    "color": {
      "values": [
