    "intents": {
      "HassGetState": {
        "one": "{{ slots.name | capitalize }} כרגע {{ state.state_with_unit }}\n",
        "one_yesno": "{% if query.matched %}\n  כן\n{% else %}\n  לא, {{ state.state_with_unit }}\n{% endif %}\n",
        "any": "{% if query.matched %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {% if match | length > 4 %}\n    כן, {{ match[:3] | join(\", \") }} ועוד {{ (match | length - 3) }}\n  {%- else -%}\n    כן,\n    {% for name in match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} ו{% endif -%}\n      {{ name }}\n    {%- endfor -%}\n  {% endif %}\n{% else %}\n  לא\n{% endif %}\n",
        "all": "{% if not query.unmatched: %}\n  כן\n{% else %}\n  {% set no_match = query.unmatched | map(attribute=\"name\") | sort | list %}\n  {% if no_match | length > 4 %}\n    לא, {{ no_match[:3] | join(\", \") }} וגם לא עוד {{ (no_match | length - 3) }}\n  {%- else -%}\n    לא,\n    {% for name in no_match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} וגם{% endif -%}\n      {{ name }}\n    {%- endfor %} לא\n  {% endif %}\n{% endif %}\n",
        "which": "{% if not query.matched %}\n  לא נמצאו\n{% else: %}\n  {% set match = query.matched | map(attribute=\"name\") | sort | list %}\n  {% if match | length > 4 %}\n    {{ match[:3] | join(\", \") }} ועוד {{ (match | length - 3) }}\n  {% else %}\n    {%- for name in match -%}\n      {% if not loop.first and not loop.last %}, {% elif loop.last and not loop.first %} ו{% endif -%}\n      {{ name }}\n    {%- endfor -%}\n  {% endif %}\n{% endif %}\n",
        "how_many": "{{ query.matched | length }}\n"
      },
      "HassTurnOff": {
        "default": "{{ slots.name }} כובה",
        "lights_area": "האורות ב{{ slots.area }} כובו",
        "fans_area": "המאווררים ב{{ slots.area }} כובו",
        "cover": "סוגר {{ slots.name }}",
