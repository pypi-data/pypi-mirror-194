    #     pprint(api_response)
    #     pprint(http_status_code)
    #     pprint(response_headers)
    # except fds.sdk.FactSetFunds.ApiException as e:
    #     print("Exception when calling FundFlowsAUMApi->get_funds_aum: %s\n" % e)

    # Get response asynchronous
    # try:
